  "dependencies": {
    "@fullpage/react-fullpage": "0.1.19",
    "classnames": "2.3.1",
    "env-cmd": "10.1.0",
    "ethers": "5.4.7",
    "lodash.chunk": "4.2.0",
    "next": "11.1.2",
    "rc-slider": "9.7.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hot-toast": "2.1.1",
